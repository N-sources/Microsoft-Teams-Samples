{
  "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
  "manifestVersion": "1.16",
  "version": "1.0.0",
  "id": "${{TEAMS_APP_ID}}",
  "packageName": "com.microsoft.teams.extension",
  "developer": {
    "name": "Teams App, Inc.",
    "websiteUrl": "https://${{TAB_DOMAIN}}",
    "privacyUrl": "https://${{TAB_DOMAIN}}/privacy",
    "termsOfUseUrl": "https://${{TAB_DOMAIN}}/termsofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Channel Group Tab",
    "full": ""
  },
  "description": {
    "short": "Simple hello world app showcasing channel and group tabs in Microsoft Teams.",
    "full": "This sample demonstrates a basic hello world app featuring channel and group tabs in Microsoft Teams."
  },
  "accentColor": "#FFFFFF",
  "configurableTabs": [
    {
      "configurationUrl": "https://${{TAB_DOMAIN}}/config",
      "canUpdateConfiguration": true,
      "scopes": [
        "team",
        "groupchat"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "localhost",
    "${{TAB_DOMAIN}}"
  ]
}