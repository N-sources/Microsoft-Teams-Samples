{
  "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
  "manifestVersion": "1.16",
  "version": "1.0.1",
  "id": "{{Manifest-id}}",
  "packageName": "com.teams.incomingwebhook",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.teams.com/privacy",
    "termsOfUseUrl": "https://www.teams.com/termsofuser"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Incoming webhook",
    "full": "Sample to show incoming webhook card"
  },
  "description": {
    "short": "Teams tab to send message cards via Incoming Webhook with HttpPOST action",
    "full": "This sample demonstrates a Teams tab to send message cards using Incoming Webhook, showcasing the HttpPOST action for interactive cards."
  },
  "accentColor": "#5558AF",
  "staticTabs": [
    {
      "entityId": "incomingwebhook",
      "name": "Incoming webhook",
      "contentUrl": "https://{{Domain-Name}}/configure-message",
      "websiteUrl": "https://{{Domain-Name}}/configure-message",
      "scopes": [
        "personal"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<<domain-name>>",
    "<<Domain-Name>>"
  ]
}